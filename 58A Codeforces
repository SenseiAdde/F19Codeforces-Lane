// 58A Codeforces simplified
#include <iostream>
using namespace std;
int main() {
    string b="hello";
    string s;
    int c=0;
    int wc=0;
    cin>>s;
    for(int i=0;i<s.size();i++){
        if(s[i]==b[c]){
            c++;
            wc++;
            if(wc==5){
                break;
            }

        }
    }
    if(wc==5){
        cout<<"YES";
    }
    else{
        cout<<"NO";
    }
    return 0;
}
